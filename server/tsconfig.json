{
  "exclude": [
    "**/*test.ts"
  ],
  "compilerOptions": {
    "noImplicitAny": true,
    "strict": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "ES2022",
    "downlevelIteration": true,
    "module": "CommonJS",
    "lib": [
      "es6"
    ],
    "outDir": "dist",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}